{
  "version": "0.6.4",
  "license": "AGPL-3.0-or-later",
  "name": "@constl/ipa",
  "author": "Julien Jean Malard-Adam",
  "type": "module",
  "types": "./dist/index.d.ts",
  "exports": "./dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "nettoyer": "rimraf dist",
    "compiler": "pnpm versioner && pnpm nettoyer && pnpm ttsc -p tsconfig.build.json",
    "test": "node --no-warnings --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand",
    "test:proc": "env MANDATAIRE=PROC pnpm test",
    "test:tous": "env MANDATAIRE=TOUS pnpm test",
    "couverture": "pnpm test -- --coverage",
    "couverture:tous": "pnpm test:tous -- --coverage",
    "formatter": "pnpm prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\" && pnpm eslint --ext .js,.ts src",
    "publier": "pnpm versioner && pnpm compiler && git commit -a -m 'version' && pnpm publish",
    "versioner": "pnpm genversion  --es6 --semi src/version.ts"
  },
  "watch": {
    "build": {
      "patterns": "./src/**/*",
      "extensions": [
        ".ts",
        ".tsx"
      ]
    },
    "postbuild": "./dist/**/*"
  },
  "devDependencies": {
    "@constl/orbit-db-types": "^1.0.5",
    "@libp2p/interface-connection-manager": "^1.3.0",
    "@libp2p/interface-pubsub": "^3.0.0",
    "@types/adm-zip": "^0.5.0",
    "@types/crypto-js": "^4.1.1",
    "@types/deep-equal": "^1.0.1",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^29.2.0",
    "@types/lodash": "^4.14.186",
    "@types/node": "^18.11.8",
    "@types/node-localstorage": "^1.3.0",
    "@types/rimraf": "^3.0.2",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "aegir": "^37.6.7",
    "bmp-js": "^0.1.0",
    "canvas": "^2.10.2",
    "core-js": "^3.26.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "image-type": "^5.1.0",
    "ipfs-core-types": "^0.13.0",
    "it-all": "^2.0.0",
    "jest": "^29.2.2",
    "keyv": "^4.5.2",
    "libp2p": "^0.40.0",
    "prettier": "^2.7.1",
    "require-json5": "^1.3.0",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.21",
    "tmp": "^0.2.1",
    "ts-jest": "^29.0.3",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "ttypescript": "^1.5.13",
    "typescript": "4.8.4",
    "typescript-transform-paths": "^3.4.4",
    "why-is-node-running": "^2.2.2"
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^10.0.0",
    "@chriscdn/promise-semaphore": "^1.0.9",
    "@constl/mandataire": "^0.1.2",
    "@libp2p/tcp": "^5.0.1",
