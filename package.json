  "scripts": {
    "nettoyer": "rimraf dist",
    "compiler": "pnpm versioner && pnpm nettoyer && pnpm tspc -p tsconfig.build.json",
    "test:node": "pnpm compiler && env MANDATAIRE=PROC pnpm aegir test -t node --cov -- --exit",
    "test:navig": "pnpm compiler && env MANDATAIRE=PROC pnpm aegir test -t browser --cov",
    "test:électron-principal": "pnpm compiler && env MANDATAIRE=PROC pnpm aegir test -t electron-main --cov",
    "test:électron-rendu": "pnpm compiler && env MANDATAIRE=PROC pnpm aegir test -t electron-renderer --cov",
    "test": "pnpm test:node && pnpm test:navig",
    "formatter": "pnpm prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\" && pnpm eslint --ext .js,.ts src",
    "publier": "pnpm versioner && pnpm compiler && (git commit -a -m 'version' || true) && pnpm publish",
    "versioner": "pnpm genversion  --es6 --semi src/version.ts"
  },
  "watch": {
    "build": {
      "patterns": "./src/**/*",
      "extensions": [
        ".ts",
        ".tsx"
      ]
    },
