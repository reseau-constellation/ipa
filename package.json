    "chokidar": "^3.5.3",
    "crypto-js": "^4.1.1",
    "deep-equal": "^2.1.0",
    "deepmerge": "^4.2.2",
    "file-saver": "^2.0.5",
    "genversion": "^3.1.1",
    "geojson-validation": "^1.0.2",
    "ipfs": "^0.65.0",
    "ipfs-core": "^0.17.0",
    "ipfsd-ctl": "^12.2.2",
    "iso-constants": "github:inverted-capital/iso-constants#patch-1",
    "it-to-buffer": "^3.0.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "multiformats": "9.9.0",
    "orbit-db": "^0.28.7",
    "orbit-db-access-controllers": "^0.3.2",
    "ssim": "^0.0.0",
    "tslib": "^2.4.1",
